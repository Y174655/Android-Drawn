<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drawn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Drawn">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.example.drawn.SanJiao"
            android:label="画三角形" >
        </activity>
        <activity
            android:name="com.example.drawn.JuXing"
            android:label="画矩形" >
        </activity>
        <activity
            android:name="com.example.drawn.Yuan"
            android:label="画圆形" >
        </activity>
        <activity
            android:name="com.example.drawn.Sin"
            android:label="Sin函数图像" >
        </activity>
        <activity
            android:name="com.example.drawn.Cos"
            android:label="cos函数图像" >
        </activity>
        <activity
            android:name="com.example.drawn.X"
            android:label="x^2函数图像" >
        </activity>
    </application>

</manifest>